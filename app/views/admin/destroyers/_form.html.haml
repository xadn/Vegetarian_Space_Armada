- form_for [:members, destroyer], :html => { :multipart => true } do |f|
  = f.error_messages
  %p
    = f.label :photo
    = f.file_field :photo
  %p
    = f.label :name
    %br
    = f.text_field :name
  %p
    = f.label :price
    %br
    = f.text_field :price
  %p
    = f.label :description
    %br
    = f.text_area :description
  %p
    = f.submit
